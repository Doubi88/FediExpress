config_version=5

[application]

config/name="FediExpress"
config/version="0.1"
run/main_scene="res://UI/splash_screen/splash.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://Logo_single.png"

[autoload]

Dialogue="*res://UI/dialogue/dialogue.tscn"
GridData="*res://globals/grid_data.gd"
MusicPlayer="*res://Audio/music_player.tscn"
GlobalServerData="*res://globals/global_server_data.gd"
MainMenu="*res://UI/pause_menu/pause_menu.tscn"
AudioSignals="*res://UI/audio_control/audio_signals.gd"
GameOverScreen="*res://UI/game_over/game_over_screen.tscn"
SceneSwitcher="*res://globals/scene_switcher.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/gut/plugin.cfg")

[gui]

theme/custom="res://theme.tres"

